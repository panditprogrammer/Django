{
    "version":1,
    "builds": [{
        "src": "djangotutorials/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb","runtime": 3.10 }
    }],
    "routes": [
        {
            "src": "build_files.sh",
            "use": "vercel/static-build",
            "dest": "djangotutorials/wsgi.py"
        }
    ]
}